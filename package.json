{
  "name": "prisma-paranoid",
  "version": "0.2.7",
  "description": "Prisma extension for soft delete (paranoid pattern)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "prisma-metadata-generator": "./src/generators/meta.js"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "format": "npx prettier --write ./**/*.{js,jsx,ts,tsx,md,json}",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EduardMcfly/prisma-paranoid.git"
  },
  "keywords": [
    "prisma",
    "soft-delete",
    "paranoid",
    "extension",
    "orm"
  ],
  "author": "EduardMcfly",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EduardMcfly/prisma-paranoid/issues"
  },
  "homepage": "https://github.com/EduardMcfly/prisma-paranoid#readme",
  "peerDependencies": {
    "@prisma/client": ">=5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@prisma/client": "^7.3.0",
    "@types/chai": "^4",
    "@types/jest": "30.0.0",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "chai": "^4",
    "eslint": "9.35.0",
    "eslint-plugin-jest": "29.0.1",
    "jest": "30.1.3",
    "prettier": "3.6.2",
    "prisma": "^7.3.0",
    "ts-jest": "29.4.1",
    "typescript": "5.9.2"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@prisma/generator-helper": "^7.3.0"
  }
}